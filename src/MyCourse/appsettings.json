{
    "Logging": {
        "LogLevel": {
          "Default": "Warning"
        }
    },
    "ConnectionStrings": {
        "Default": "Data Source=Data/MyCourse.db",
        "DistributedCache": "Data Source=(localdb)\\MSSQLLocalDb;Integrated Security=True"
    },
    "DistributedCache": {
        "SchemaName": "dbo",
        "TableName": "SQLCache"
    },
    "Courses": {
        "PerPage": 10,
        "Order": {
                "By": "Rating",
                "Ascending": false,
				"Allow": ["Title", "Rating", "CurrentPrice"]
        }
    }
}